{
    "name": "bitnoise/mailer-bundle",
    "type": "symfony-bundle",
    "description": "",
    "keywords": [ "bitnoise", "mailer"],
    "homepage": "https://github.com/Bitnoise/BtnMailerBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Bitnoise Team",
            "homepage": "http://www.bitnoi.se/",
            "email": "labs@bitnoi.se"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "bitnoise/base-bundle": "2.0.*"
    },
    "config": {
        "preferred-install": "source"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://bitnoise.github.io/packages/"
        }
    ],
    "autoload": {
        "psr-4": { "Btn\\MailerBundle\\": "src/Btn/MailerBundle/" }
    },
    "extra": {
        "branch-alias": {
            "dev-2.0": "2.0-dev"
        }
    }
